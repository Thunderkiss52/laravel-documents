{
    "name": "Thunderkiss52/laravel-documents",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "Thunderkiss52/laravel-permission": "dev-main",
        "phpoffice/phpspreadsheet": "^4.0",
        "phpoffice/phpword": "^1.3",
        "lorisleiva/laravel-actions": "^2.8",
        "spatie/temporary-directory": "^2.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thunderkiss52\\LaravelDocuments\\DocumentsProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Thunderkiss52\\LaravelDocuments\\": "src/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "baseline": "vendor/bin/phpstan analyse --generate-baseline",
        "test": "vendor/bin/pest"
    },
    "authors": [
        {
            "name": "Thunderkiss52",
            "email": "Thunderkiss52@gmail.com"
        }
    ]
}